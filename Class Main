import javax.swing.*;

public class Main {
    public static void main(String[] args) {
        javax.swing.SwingUtilities.invokeLater(new Runnable() {
            public void run() {
                createAndShowGUI();
            }
        });
    }

    private static void createAndShowGUI() {
        AplikasiKlinik aplikasiKlinik = new AplikasiKlinik();
        aplikasiKlinik.setTitle("Aplikasi Klinik By Nurul Aulia Salsabila");
        aplikasiKlinik.setSize(500, 300);
        aplikasiKlinik.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        aplikasiKlinik.setVisible(true);
    }
}
